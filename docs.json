{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Dagger",
  "colors": {
    "primary": "#1a1833",
    "light": "#f8f4ef",
    "dark": "#131126"
  },
  "favicon": "/favicon.png",
  "banner": {
    "dismissible": true,
    "content": "Join Our Engineering Deep Dive on Agentic CI [Register Now](https://dagger.io/deep-dives/agentic-ci)"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "groups": [
          {
            "group": "Getting Started",
            "icon": "house",
            "pages": [
              "index",
              "installation",
              "quickstart/workflow",
              "quickstart/core-concepts",
              "ide-setup"
            ]
          },
          {
            "group": "Use Cases",
            "icon": "briefcase-conveyor-belt",
            "pages": ["use-cases/modern-ci", "use-cases/agentic-ci"]
          },
          {
            "group": "Components",
            "icon": "list-check",
            "pages": [
              "components/overview",
              "components/functions",
              {
                "group": "Objects",
                "icon": "chart-no-axes-gantt",
                "pages": [
                  "components/container",
                  "components/directory",
                  "components/file",
                  "components/llm",
                  "components/secret",
                  "components/service",
                  "components/environment"
                ]
              }
            ]
          },
          {
            "group": "Engine & Runtime",
            "icon": "list-check",
            "pages": [
              "engine/overview",
              "engine/local-development",
              "engine/configuration",
              "engine/performance",
              "engine/troubleshooting",
              {
                "group": "CI Integration",
                "icon": "blocks",
                "pages": [
                  "engine/ci-integration/github-actions",
                  "engine/ci-integration/gitlab-ci",
                  "engine/ci-integration/azure-pipelines"
                ]
              }
            ]
          }
        ]
      },
      {
        "tab": "Examples",
        "groups": [
          {
            "group": "Overview",
            "icon": "lightbulb",
            "pages": ["examples/code", "examples/demos", "examples/livestreams"]
          }
        ]
      },
      {
        "tab": "Reference",
        "icon": "code",
        "groups": [
          {
            "group": "CLI Reference",
            "pages": ["reference/cli/overview"]
          },
          {
            "group": "API Documentation",
            "pages": [
              "reference/api/graphql-api",
              "reference/api/core-types",
              "reference/api/function-registry"
            ]
          },
          {
            "group": "SDKs",
            "pages": [
              "reference/sdk/overview",
              "reference/sdk/go",
              "reference/sdk/typescript",
              "reference/sdk/python",
              "reference/sdk/php"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Community",
          "href": "https://dagger.io/community",
          "icon": "discord"
        },
        {
          "anchor": "Blog",
          "href": "https://dagger.io/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@dagger.io"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Try Dagger Cloud",
      "href": "https://dagger.io/cloud"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/dagger_io",
      "github": "https://github.com/dagger",
      "linkedin": "https://www.linkedin.com/company/dagger-io"
    }
  },
  "background": {
    "color": {
      "light": "#f8f4ef",
      "dark": "#131126"
    }
  },
  "styling": {
    "eyebrows": "section",
    "codeblocks": "system"
  },
  "icons": {
    "library": "lucide"
  }
}
